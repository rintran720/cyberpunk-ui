{
  "name": "@votekio/cyberpunk-ui",
  "version": "0.1.0",
  "description": "A 3D-styled UI component library built with TailwindCSS",
  "author": "John Tran <rintran720@gmail.com>",
  "homepage": "https://cyberpunk-ui-tawny.vercel.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/rintran720/cyberpunk-ui.git"
  },
  "bugs": {
    "url": "https://github.com/rintran720/cyberpunk-ui/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./styles": {
      "import": "./dist/styles/globals.css",
      "require": "./dist/styles/globals.css"
    },
    "./styles/globals.css": "./dist/styles/globals.css",
    "./styles/themes.css": "./dist/styles/themes.css",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "tsup",
    "build:storybook": "storybook build",
    "prepublishOnly": "npm run build && npm run type-check",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.4.5",
    "@storybook/addon-interactions": "^8.4.5",
    "@storybook/addon-links": "^8.4.5",
    "@storybook/blocks": "^8.4.5",
    "@storybook/react": "^8.4.5",
    "@storybook/react-vite": "^8.4.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "postcss": "^8.4.49",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.4.5",
    "tailwindcss": "^3.4.15",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vite": "^6.0.1"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.5"
  },
  "keywords": [
    "react",
    "ui",
    "3d",
    "components",
    "tailwindcss",
    "design-system"
  ],
  "license": "MIT"
}
